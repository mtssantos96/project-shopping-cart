    ],
    "testMatch": [
      "**/tests/**/*.test.js?(x)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtssantos96/project-shopping-cart.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mtssantos96/project-shopping-cart/issues"
  },
  "homepage": "https://github.com/mtssantos96/project-shopping-cart#readme",
  "dependencies": {
    "eslint-config-trybe-fundamentals": "1.1.0",
    "jest": "24.9.0",
    "mocha": "7.1.1",
    "mochawesome": "5.0.0",
    "mochawesome-merge": "4.0.3",
    "mochawesome-report-generator": "4.1.0",
    "stylelint": "13.8.0",
    "stylelint-config-standard": "20.0.0"
  }
